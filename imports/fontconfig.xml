<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "urn:fontconfig:fonts.dtd">
<!-- /etc/fonts/fonts.conf file to configure system font access -->
<fontconfig>
    <match target="pattern">
        <test name="family" compare="contains">
            <string>Noto Serif CJK</string>
        </test>
        <edit name="weight">
            <const>medium</const>
        </edit>
    </match>
</fontconfig>
